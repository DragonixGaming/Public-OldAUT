	game:GetService("ReplicatedStorage").Stand:FireServer("HolyPotPlatinumStand",50,1000000)
	game:GetService("ReplicatedStorage").Stand:FireServer("GoldExperienceRequiemStand",50,1000000)
	game:GetService("ReplicatedStorage").Stand:FireServer("KingCrimsonRequiemStand",50,1000000)
	game:GetService("ReplicatedStorage").Stand:FireServer("KnightPotStand",50,1000000,"\226\153\146\226\153\142\226\153\136\226\153\138\226\153\147\226\153\136\226\153\137\226\153\136\226\153\136\226\153\138")
	game:GetService("ReplicatedStorage").Stand:FireServer("MIUStand",5,1000000)
	game:GetService("ReplicatedStorage").Stand:FireServer("HeavenStarPlatinum",50,1000000)
	game:GetService("ReplicatedStorage").Stand:FireServer("StevePlatinum",50,1000000)

end

local mouse = game.Players.LocalPlayer:GetMouse()
	_G.contc = mouse.KeyDown:Connect(function(key)
		if key == 'e' then
			if stand == 0 then
				_G.func()
			else
				_G.func2()
			end
		elseif key == 'j' then
			if stand == 0 then
				_G.strong()
			else
				_G.strong2()
			end
		elseif key == 'r' then
			if stand == 0 then
				_G.kick()
			else
				_G.kick2()
			end
		elseif key == 'b' then
			if stand == 0 then
				_G.lepunch()
			else
				_G.lepunch2()
			end
		elseif key == 'y' then
			if stand == 0 then
				_G.ledown()
			else
				_G.ledown2()
			end
		elseif key == 't' then
			if stand == 0 then
				_G.grab()
			else
				_G.control()
			end
		elseif key == 'u' then
			if stand == 0 then
				_G.poison()
			else
				_G.control()
			end
		elseif key == 'l' then
			--if stand == 0 then
				_G.freeze()
			--end
		elseif key == 'n' then
			--if stand == 0 then
				_G.taunt()
			--end
		elseif key == 'k' then
		--	if stand == 0 then
				_G.nou()
		--	end
		elseif key == 'q' then
		--	if stand == 0 then
				_G.costume()
		--	end
		elseif key == 'h' then
			--if stand == 0 then
				_G.bonk()
			--end
		elseif key == '=' then
			if stand == 0 then
				_G.destroy()
			end
		elseif key == '-' then
			_G.spin()
			if stand == 0 then
				stand = 1
			else
				stand = 0
			end
		elseif key == 'x' then
			--if stand == 0 then
				_G.backstab()
			--end
		elseif key == 'v' then
			--if stand == 0 then
				game.Players.LocalPlayer.Character:MoveTo(mouse.Hit.p)
			--end
		elseif key == "f" then
			_G.freezeall()
		end
		
	end)

	print('h') -- 4774749466
