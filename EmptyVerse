		game:GetService("ReplicatedStorage").Taunt:FireServer(game:GetService("Players").LocalPlayer.Character.Head,"rbxassetid://1228181736",10,0,0)
			if debounce == false then
				debounce = true
				print(hit.Parent.Parent.Name)
